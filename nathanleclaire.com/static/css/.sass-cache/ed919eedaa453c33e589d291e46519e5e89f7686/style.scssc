/* ==|== primary styles =====================================================
   Author: Nathan LeClaire
   ========================================================================== */

$gridwidth: 60%;
$colwidth: $gridwidth / 12;

@import url(http://fonts.googleapis.com/css?family=Mate+SC);
@import url(http://fonts.googleapis.com/css?family=Monofett);

body {
  background: url(../img/arches.png);
}

img#githubribbon { 
    position: absolute; 
    top: 0; 
    right: 0; 
    border: 0; 
} 

div#headerbg {
  width: 100%;
  padding-bottom: .7%;
  background-color: rgba(255, 255, 255, 0.85);
}

#header {

  font-family: 'Mate SC', 'Lucida Grande', serif;
}

#logo {
  font-family: 'Monofett', 'Courier New', 'Helvetica', sans-serif;
  font-size: 3em;
  line-height: 1.0;
  margin: {
    bottom: 0.2%;
    left: 4%;
  }
}

#main {
  height: 100%;
}

.blogg {
  border-radius: 14px;
  background-color: rgba(255, 255, 255, 0.7);
  width: 60%;
  margin: {
    left: 4%;
  }
  -webkit-box-shadow: 5px -6px 5px rgba(12, 0, 12, 0.53);
  -moz-box-shadow:    5px -6px 5px rgba(12, 0, 12, 0.53);
  box-shadow:         5px -6px 5px rgba(12, 0, 12, 0.53);
  border: 2px solid rgba(12, 0, 12, 0.53);
}

p {
  font-family: serif;
  font-size: 16px;
  margin: {
    top: 0%;
    bottom: 0%;
  }
}

ul#frontpage {
  list-style-type: none;
  font-size: 2em;
  line-height: 1.0;
  padding: 0%;
}

.blog-entry {
  padding:{
    top: 1.0%;
    left: 2.2%;
    right: 2.2%;
    bottom: 2.2%;
  }
}

h2 {
  line-height: 1.0;
  margin: {
    top: 0%;
    bottom: 0.9%;
    left: $colwidth;
  }
  font-size: 1em;
  font-family: 'Mate SC', 'Lucida Grande', serif;
}

h3 {
  text-decoration: underline;
  margin: {
    bottom: 0.2%;
    top: 0%;
  }
  color: #FF4500;
  font-size: 2em;
  font-family: 'Mate SC', 'Lucida Grande', serif;
}

a:visted {
  color:blue;
}

img {
  max-width: 100%;
  border-radius: 14px;
}


