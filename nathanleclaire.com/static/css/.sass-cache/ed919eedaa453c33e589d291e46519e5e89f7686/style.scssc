/* ==|== primary styles =====================================================
   Author: Nathan LeClaire
   ========================================================================== */

$gridwidth: 80%;
$colwidth: $gridwidth / 12;

@import url(http://fonts.googleapis.com/css?family=Mate+SC);
@import url(http://fonts.googleapis.com/css?family=Monofett);

body {
  background: url(../img/arches.png);
}

img#githubribbon { 
    position: absolute; 
    top: 0; 
    right: 0; 
    border: 0; 
} 

div#headerbg {
  width: 100%;
  background-color: rgba(255, 255, 255, 0.7);
}

#header {
  font-family: 'Mate SC', 'Lucida Grande', serif;
}

#logo {
  font-family: 'Monofett', 'Courier New', 'Helvetica', sans-serif;
  font-size: 4em;
  line-height: 1.0;
  margin: {
    bottom: 0.2%;
    left: $colwidth;
  }
}

#main {
  height: 100%;
}

p {
  font-family: serif;
  font-size: 16px;
  margin: {
    left: $colwidth*.5;
    top: 0%;
    bottom: 0%;
  }
}

ul#frontpage {
  list-style-type: none;
  font-size: 2em;
  line-height: 1.0;
  padding: 0%;
}

.blog-entry {
  width: 60%;
  margin: {
    left: 3%;
  }
}

h2 {
  line-height: 1.0;
  margin: {
    top: 0%;
    bottom: 0.2%;
    left: $colwidth;
  }
  font-size: 2em;
  font-family: 'Mate SC', 'Lucida Grande', serif;
}

h3 {
  text-decoration: underline;
  margin: {
    left: $colwidth * 2;
    bottom: 0.2%;
    top: 0%;
  }
  color: #FF4500;
  font-size: 2em;
  font-family: 'Mate SC', 'Lucida Grande', serif;
}

a:visted {
  color:blue;
}


